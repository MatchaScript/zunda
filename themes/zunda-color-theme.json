  "semanticHighlighting": true,
  "semanticTokenColors": {
    "comment": "#fff9",
    "string": "#E96B5D",
    "number": "#C2B363",
    "keyword": { "bold": true, "foreground": "#E9629E" },
    "boolean": { "bold": true, "foreground": "#E9629E" },
    "builtinType": { "bold": true, "foreground": "#E9629E" },
    "macro": "#E78D3F",
    "type": "#8EDACC",
    "class": "#8EDACC",
    "struct": "#8EDACC",
    "enum": "#8EDACC",
    "enumMember": "#8EDACC",
    "typeAlias": "#8EDACC",
    "typeParameter": "#8EDACC",
    "union": "#8EDACC",
    "variable": "#60AE9E",
    "member": "#60AE9E",
    "parameter": "#60AE9E",
    "property": "#60AE9E",
    "lifetime": "#60AE9E",
    "variable.constant": "#60AE9E",
    "member.constant": "#60AE9E",
    "parameter.constant": "#60AE9E",
    "property.constant": "#60AE9E",
    "lifetime.constant": "#60AE9E",
    "interface": "#C4A4E9",
    "function": "#9E6ED7",
    "punctuation": "#D6DBEA",
    "operator": "#D6DBEA",
    "type.declaration": "#56C9E9",
    "class.declaration": "#56C9E9",
    "struct.declaration": "#56C9E9",
    "enum.declaration": "#56C9E9",
    "enumMember.declaration": "#56C9E9",
    "typeAlias.declaration": "#56C9E9",
    "typeParameter.declaration": "#56C9E9",
    "union.declaration": "#56C9E9",
    "*.declaration": "#3E9DBA"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#fff9",
        "fontStyle": ""
      }
    },
    {
      "scope": ["punctuation.definition.string", "string"],
      "settings": {
        "foreground": "#E96B5D",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.numeric", "keyword.other.unit", "support.constant"],
      "settings": {
        "foreground": "#C2B363",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "constant.language",
        "entity.name.tag",
        "keyword",
        "storage.modifier",
        "storage.type",
        "support.type.primitive",
        "variable.language"
      ],
      "settings": {
        "foreground": "#E9629E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function.macro",
        "keyword.control.directive",
        "keyword.control.preprocessor",
        "punctuation.definition.preprocessor"
      ],
      "settings": {
        "foreground": "#E78D3F",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#6699ff",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.type.class.std.rust",
        "storage.type.cs",
        "support.type"
      ],
      "settings": {
        "foreground": "#8EDACC",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "punctuation.definition.variable",
        "punctuation.support.type.property-name",
        "storage.modifier.lifetime",
        "support.type.property-name",
        "variable"
      ],
      "settings": {
        "foreground": "#60AE9E",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#9E6ED7",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#56C9E9",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.function.definition",
        "entity.name.function.go",
        "entity.name.type.interface",
        "entity.name.type.namespace",
        "entity.name.variable",
        "variable.other.assignment"
      ],
      "settings": {
        "foreground": "#3E9DBA",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "fenced_code.block.language",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.heading.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.markdown",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.quote.begin.markdown",
        "punctuation.definition.raw.markdown",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown"
      ],
      "settings": {
        "foreground": "#60AE9E",
        "fontStyle": ""
      }
    },
    {
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#D6DBEA",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.section"],
      "settings": {
        "foreground": "#D6DBEA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#D6DBEA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#D6DBEA",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation", "keyword.operator"],
      "settings": {
        "foreground": "#D6DBEA",
        "fontStyle": ""
      }
    }
  ]
}
